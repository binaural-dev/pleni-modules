<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="find_us_view_form" model="ir.ui.view">
    <field name="name">find.us.view.form</field>
    <field name="model">find.us</field>
    <field name="arch" type="xml">
      <form string="¿Cómo llegó a nosotros?">
        <sheet>
          <group>
            <field name="type_name" />
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="find_us_view_tree" model="ir.ui.view">
    <field name="name">find_us.view.tree</field>
    <field name="model">find.us</field>
    <field name="arch" type="xml">
      <tree string="Registros de ¿Cómo llegó a nosotros?">
        <field name="type_name" />
        <field name="create_date" />
      </tree>
    </field>
  </record>

  <record id="find_us_action" model="ir.actions.act_window">
    <field name="name">Registros de ¿Cómo llegó a nosotros?</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">find.us</field>
    <field name="view_mode">tree,form</field>
  </record>
</odoo>