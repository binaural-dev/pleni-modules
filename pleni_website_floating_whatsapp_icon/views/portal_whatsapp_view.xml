<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="ChatWithATC" name="Chat with ATC">
        <xpath expr="//div[hasclass('s_share')]" position="inside">
            <a t-attf-href="https://web.whatsapp.com/send?phone=#{website.atc_mobile_number}" class="atc_whatsapp_web" target="_blank">
                <i class="fa fa-whatsapp atc-icon" />
            </a>
        </xpath>
    </template>
</odoo>